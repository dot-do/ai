{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "paths": {
      "~/*": ["./app/*"]
    },
    "types": ["@cloudflare/workers-types", "vite/client"]
  },
  "include": ["app/**/*", "react-router.config.ts"],
  "exclude": ["node_modules", "build", ".react-router"]
}
