{
  "name": "agents.org.ai",
  "version": "1.0.0",
  "description": "Agent type definitions and implementations for the .do platform based on OpenAI AgentKit",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": ".do Platform Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/platform.git",
    "directory": "ai/packages/agents.org.ai"
  },
  "bugs": "https://github.com/dot-do/platform/issues",
  "homepage": "https://agents.org.ai",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./pdm": {
      "types": "./dist/pdm.d.ts",
      "default": "./dist/pdm.js"
    },
    "./vapi": {
      "types": "./dist/vapi.d.ts",
      "default": "./dist/vapi.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "typescript": "^5.7.3",
    "vitest": "^3.2.3"
  },
  "keywords": [
    "agents",
    "ai",
    "openai",
    "agentkit",
    "product-manager",
    "autonomous"
  ]
}
