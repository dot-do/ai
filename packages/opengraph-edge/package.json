{
  "name": "opengraph-edge",
  "version": "0.1.0",
  "description": "Generate Open Graph images from JSX using Satori and resvg on Cloudflare Workers",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "satori": "^0.12.0",
    "@resvg/resvg-wasm": "^2.6.2"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "react": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  },
  "keywords": [
    "opengraph",
    "og-image",
    "social-cards",
    "satori",
    "cloudflare-workers",
    "jsx-to-png"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/ai.git",
    "directory": "packages/opengraph-edge"
  },
  "license": "MIT"
}
