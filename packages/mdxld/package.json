{
  "name": "mdxld",
  "version": "2.0.0",
  "description": "Enable $id and $type in MDX frontmatter for linked data",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "mdxld": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./schemas/Theme": {
      "import": "./dist/schemas/Theme.js",
      "types": "./dist/schemas/Theme.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "mdx",
    "linked-data",
    "frontmatter",
    "json-ld"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.6",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "gray-matter": "^4.0.3",
    "oauth.do": "workspace:*",
    "ora": "^8.0.1",
    "yaml": "^2.6.1",
    "zod": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
