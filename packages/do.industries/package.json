{
  "name": "do.industries",
  "version": "0.0.1",
  "description": "Types and constants for all .do domains and industries",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./data": {
      "import": "./dist/data/index.js",
      "types": "./dist/data/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "keywords": [
    "domains",
    "industries",
    "do",
    "services",
    "types"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@dotdo/tsconfig": "workspace:*",
    "@types/node": "^22.10.6",
    "typescript": "^5.7.3",
    "vitest": "^3.2.3"
  }
}
