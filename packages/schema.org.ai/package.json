{
  "name": "schema.org.ai",
  "version": "0.1.0",
  "description": "Schema.org vocabulary package - 817 types, 1,518 properties for semantic AI applications",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "fetch": "tsx src/scripts/fetch-vocabulary.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "tsx": "catalog:"
  },
  "keywords": [
    "schema.org",
    "vocabulary",
    "semantic-web",
    "linked-data",
    "ontology"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/ai.git",
    "directory": "packages/schema.org.ai"
  },
  "license": "MIT"
}
