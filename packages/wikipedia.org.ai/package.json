{
  "name": "wikipedia.org.ai",
  "version": "0.1.0",
  "description": "Wikipedia structured data extraction - canonical entity references from Wikipedia articles",
  "type": "module",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": "./src/index.js",
    "./types": "./src/types/index.js",
    "./data": "./src/data/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "import": "tsx scripts/import.ts",
    "import:full": "tsx scripts/import.ts --full"
  },
  "keywords": [
    "wikipedia",
    "knowledge-graph",
    "semantic-web",
    "structured-data",
    "entities",
    "wtf_wikipedia"
  ],
  "license": "MIT",
  "dependencies": {
    "dumpster-dive": "^5.6.3",
    "wtf_wikipedia": "^10.3.2"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "vitest": "catalog:",
    "tsx": "catalog:"
  }
}
