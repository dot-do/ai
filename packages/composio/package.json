{
  "name": "@dotdo/composio",
  "version": "1.0.0",
  "description": "Composio client for .do platform with retry logic and full TypeScript support",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Nathan Clevenger <nathanclevenger@users.noreply.github.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/platform.git",
    "directory": "ai/packages/composio"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "default": "./dist/errors.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "composio",
    "integrations",
    "ai",
    "sdk",
    "automation",
    "cloudflare"
  ],
  "devDependencies": {
    "typescript": "^5.8.3",
    "vitest": "^2.1.8"
  }
}
