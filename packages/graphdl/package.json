{
  "name": "graphdl",
  "version": "1.0.0",
  "description": "Semantic graph types with $.Subject.predicate.Object patterns",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./builders": {
      "import": "./dist/builders/index.js",
      "types": "./dist/builders/index.d.ts"
    },
    "./types/complete": {
      "import": "./dist/types/complete.js",
      "types": "./dist/types/complete.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "PUBLISHING.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "ingest": "tsx scripts/ingest.ts",
    "ingest:naics": "tsx scripts/ingest.ts naics",
    "ingest:apqc": "tsx scripts/ingest.ts apqc",
    "ingest:unedi": "tsx scripts/ingest.ts unedi",
    "ingest:zapier": "tsx scripts/ingest.ts zapier",
    "enrich": "tsx scripts/enrich.ts",
    "enrich:gpt5": "tsx scripts/enrich/gpt5.ts",
    "prepublishOnly": "pnpm build && pnpm typecheck && pnpm test run"
  },
  "keywords": [
    "semantic",
    "graph",
    "business-as-code",
    "linked-data",
    "typescript",
    "schema-org",
    "gs1",
    "onet",
    "naics",
    "apqc",
    "edi",
    "tree-shakeable",
    "esm",
    "zero-dependency"
  ],
  "author": "dot-do",
  "license": "MIT",
  "engines": {
    "node": ">=14.8.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/platform.git",
    "directory": "ai/packages/graphdl"
  },
  "bugs": {
    "url": "https://github.com/dot-do/platform/issues"
  },
  "homepage": "https://github.com/dot-do/platform/tree/main/ai/packages/graphdl#readme",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "catalog:",
    "js-yaml": "^4.1.0",
    "nanoid": "^5.0.8",
    "openai": "^6.3.0",
    "tsx": "^4.19.2",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "business-as-code": "workspace:*",
    "do.industries": "workspace:*",
    "gs1.org.ai": "workspace:*",
    "mdx.org.ai": "workspace:*",
    "schema.org.ai": "workspace:*",
    "soc.org.ai": "workspace:*"
  },
  "peerDependenciesMeta": {
    "schema.org.ai": {
      "optional": true
    },
    "gs1.org.ai": {
      "optional": true
    },
    "soc.org.ai": {
      "optional": true
    },
    "mdx.org.ai": {
      "optional": true
    },
    "do.industries": {
      "optional": true
    },
    "business-as-code": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
