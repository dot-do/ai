{
  "name": "gs1.org.ai",
  "version": "0.0.1",
  "description": "GS1 EPCIS and CBV types with tree-shakeable exports",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./types/*": {
      "import": "./dist/types/*.js",
      "types": "./dist/types/*.d.ts"
    },
    "./data": {
      "import": "./dist/data/index.js",
      "types": "./dist/data/index.d.ts"
    },
    "./data/*": {
      "import": "./dist/data/*.js",
      "types": "./dist/data/*.d.ts"
    }
  },
  "scripts": {
    "import": "tsx scripts/import.ts",
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run"
  },
  "keywords": [
    "gs1",
    "epcis",
    "cbv",
    "supply-chain",
    "traceability"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
