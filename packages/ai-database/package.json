{
  "name": "ai-database",
  "version": "0.1.0",
  "description": "Semantic database operations with $.Subject.predicate.Object patterns for Business-as-Code",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./payload": {
      "types": "./dist/payload/index.d.ts",
      "import": "./dist/payload/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "database",
    "semantic",
    "graph",
    "crud",
    "relationships",
    "business-as-code",
    "schema.org",
    "linked-data"
  ],
  "dependencies": {
    "zod": "catalog:"
  },
  "peerDependencies": {
    "@dotdo/platform": "workspace:*",
    "@payloadcms/db-do-sqlite": "workspace:*",
    "payload": "*"
  },
  "peerDependenciesMeta": {
    "@payloadcms/db-do-sqlite": {
      "optional": true
    }
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251011.0",
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/platform.git",
    "directory": "ai/packages/ai-database"
  },
  "homepage": "https://database.do",
  "license": "MIT"
}
