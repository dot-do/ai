{
  "name": "@mdxld/zod",
  "version": "0.0.1",
  "description": "Zod schemas and validation utilities for MDXLD",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./schemas": {
      "import": "./dist/schemas.js",
      "types": "./dist/schemas.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run"
  },
  "keywords": [
    "mdx",
    "linked-data",
    "frontmatter",
    "json-ld",
    "zod",
    "validation",
    "schema"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "mdxld": "workspace:*",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
