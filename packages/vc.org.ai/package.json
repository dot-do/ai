{
  "name": "vc.org.ai",
  "version": "0.1.0",
  "description": "Venture capital and startup data from Crunchbase Open Data - companies, funding rounds, investors, acquisitions",
  "type": "module",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": "./src/index.js",
    "./types": "./src/types/index.js",
    "./data": "./src/data/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "import": "tsx scripts/import.ts",
    "import:sample": "tsx scripts/import.ts --sample"
  },
  "keywords": [
    "crunchbase",
    "venture-capital",
    "startups",
    "funding",
    "investors",
    "acquisitions"
