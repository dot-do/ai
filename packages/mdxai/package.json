{
  "name": "mdxai",
  "version": "2.0.0",
  "description": "Simplified AI-powered MDX development tool using OpenRouter and react-ink",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "mdxai": "./dist/cli-commander.js",
    "mdxai-agent": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "mdx",
    "ai",
    "openrouter",
    "gpt-5",
    "react-ink",
    "bun",
    "agentic"
  ],
  "author": "Nathan Clevenger <nathanclevenger@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/openai": "^1.0.0",
    "@anthropic-ai/sdk": "catalog:",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "ai": "^4.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "gray-matter": "^4.0.3",
    "ink": "^5.0.1",
    "mdxdb": "workspace:*",
    "mdxld": "workspace:*",
    "oauth.do": "workspace:*",
    "ora": "^8.0.1",
    "react": "^18.3.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.6",
    "@types/react": "^18.3.18",
    "typescript": "^5.8.3",
    "vitest": "^2.1.8"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
