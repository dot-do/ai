{
  "name": "soc.org.ai",
  "version": "0.1.0",
  "description": "O*NET SOC (Standard Occupational Classification) vocabulary - 923 occupations + tasks, skills, abilities, knowledge, work activities, tools, technology",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "fetch": "tsx src/scripts/fetch-onet.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "tsx": "catalog:"
  },
  "keywords": [
    "onet",
    "soc",
    "occupations",
    "skills",
    "abilities",
    "tasks",
    "work-activities",
    "labor-market"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/ai.git",
    "directory": "packages/soc.org.ai"
  },
  "license": "MIT"
}
