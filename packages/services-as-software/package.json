{
  "name": "services-as-software",
  "version": "0.0.1",
  "description": "Services-as-Software abstractions for building and deploying autonomous services",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./api": {
      "import": "./dist/api/index.js",
      "types": "./dist/api/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "keywords": [
    "services-as-software",
    "microservices",
    "apis",
    "serverless",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "business-as-code": "workspace:*",
    "do.industries": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.10.6",
    "typescript": "^5.7.3",
    "vitest": "^3.2.3"
  }
}
