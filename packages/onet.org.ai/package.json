{
  "name": "onet.org.ai",
  "version": "0.1.0",
  "description": "O*NET Complete Database - meta-package providing unified access to all O*NET vocabularies",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./soc": {
      "import": "soc.org.ai",
      "types": "soc.org.ai"
    },
    "./tech": {
      "import": "tech.org.ai",
      "types": "tech.org.ai"
    },
    "./tools": {
      "import": "tools.org.ai",
      "types": "tools.org.ai"
    },
    "./tasks": {
      "import": "tasks.org.ai",
      "types": "tasks.org.ai"
    },
    "./jobs": {
      "import": "jobs.org.ai",
      "types": "jobs.org.ai"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "soc.org.ai": "workspace:*",
    "jobs.org.ai": "workspace:*",
    "tasks.org.ai": "workspace:*",
    "tools.org.ai": "workspace:*",
    "tech.org.ai": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "@vitest/coverage-v8": "catalog:"
  },
  "keywords": [
    "onet",
    "occupations",
    "skills",
    "tasks",
    "tools",
    "technology",
    "labor-market",
    "workforce"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/platform.git",
    "directory": "ai/packages/onet.org.ai"
  },
  "license": "MIT"
}
