{
  "name": "ai-functions",
  "version": "0.1.0",
  "description": "Function registry, execution, and management for Business-as-Code with AI-delivered Services-as-Software",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "functions",
    "serverless",
    "execution",
    "registry",
    "business-as-code",
    "services-as-software",
    "ai"
  ],
  "dependencies": {
    "zod": "catalog:"
  },
  "peerDependencies": {
    "graphdl": "*",
    "business-as-code": "*",
    "services-as-software": "*",
    "ai-sandbox": "*"
  },
  "peerDependenciesMeta": {
    "graphdl": {
      "optional": true
    },
    "business-as-code": {
      "optional": true
    },
    "services-as-software": {
      "optional": true
    },
    "ai-sandbox": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/platform.git",
    "directory": "ai/packages/ai-functions"
  },
  "homepage": "https://functions.do",
  "license": "MIT"
}
