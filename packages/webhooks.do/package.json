{
  "name": "webhooks.do",
  "version": "0.1.0",
  "description": "Webhook verification and signing utilities for .do platform",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "webhooks",
    "signature",
    "verification",
    "hmac",
    "github",
    "stripe",
    "workos",
    "zapier"
  ],
  "author": "Nathan Clevenger",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
