{
  "name": "ai-sandbox",
  "version": "0.1.1",
  "description": "Open-source primitive for creating isolated execution sandboxes using Cloudflare Worker Loaders",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "sandbox",
    "worker-loader",
    "code-execution",
    "isolation"
  ],
  "author": ".do",
  "license": "MIT",
  "dependencies": {
    "@cloudflare/workers-types": "^4.20251011.0"
  },
  "devDependencies": {
    "typescript": "^5.7.3",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "@cloudflare/workers-types": "^4.0.0"
  }
}
