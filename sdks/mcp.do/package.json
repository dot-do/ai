{
  "name": "mcp.do",
  "version": "1.0.0",
  "description": "TypeScript SDK for mcp.do - Model Context Protocol server integration testing",
  "author": "dot-do",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "mcp": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "TEST_URL=https://mcp.do vitest run --config vitest.e2e.config.ts",
    "test:e2e:local": "TEST_URL=http://localhost:8787 vitest run --config vitest.e2e.config.ts",
    "test:integration": "TEST_URL=http://localhost:8787 vitest run --config vitest.e2e.config.ts --grep 'MCP'",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "sdk",
    "typescript",
    "cloudflare",
    "workers",
    "database",
    "integration",
    "mcp.do"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dot-do/platform.git",
    "directory": "ai/sdks/mcp.do"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "catalog:",
    "oauth.do": "workspace:*",
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "dotenv": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
