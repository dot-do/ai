{
  "name": "errors.do",
  "version": "1.0.0",
  "description": "TypeScript SDK for errors.do - error tracking and monitoring client library",
  "author": "dot-do",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "TEST_URL=https://errors.do vitest run --config vitest.e2e.config.ts",
    "test:e2e:local": "TEST_URL=http://localhost:8787 vitest run --config vitest.e2e.config.ts",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "errors",
    "error-tracking",
    "monitoring",
    "observability",
    "sdk",
    "typescript",
    "cloudflare",
    "workers",
    "errors.do"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dot-do/platform.git",
    "directory": "ai/sdks/errors.do"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
