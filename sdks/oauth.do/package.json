{
  "name": "oauth.do",
  "version": "0.1.0",
  "description": "Open-source OAuth and authentication client SDK for React and JavaScript",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.mjs",
      "require": "./dist/server.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    },
    "./cli": {
      "types": "./cli/index.ts",
      "import": "./cli/index.ts"
    },
    "./cli/keychain": {
      "types": "./cli/keychain.ts",
      "import": "./cli/keychain.ts"
    },
    "./cli/oauth": {
      "types": "./cli/oauth.ts",
      "import": "./cli/oauth.ts"
    },
    "./cli/stdio": {
      "types": "./cli/stdio.ts",
      "import": "./cli/stdio.ts"
    },
    "./cli/apikey": {
      "types": "./cli/apikey.ts",
      "import": "./cli/apikey.ts"
    },
    "./cli/types": {
      "types": "./cli/types.ts",
      "import": "./cli/types.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage",
    "lint": "eslint src",
    "format": "prettier --write src",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "better-auth": "^1.3.0",
    "jose": "^5.9.6",
    "@napi-rs/keyring": "^1.2.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "open": "^10.1.0"
  },
  "peerDependencies": {
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "keywords": [
    "oauth",
    "auth",
    "authentication",
    "better-auth",
    "react",
    "hooks",
    "client",
    "sdk",
    "open-source"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/platform.git",
    "directory": "ai/sdks/oauth.do"
  },
  "author": ".do Platform",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
