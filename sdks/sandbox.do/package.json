{
  "name": "sandbox.do",
  "version": "0.1.0",
  "description": "TypeScript SDK for accessing the Cloudflare Sandbox API",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cloudflare",
    "sandbox",
    "worker-loader",
    "code-execution",
    "sdk"
  ],
  "author": ".do",
  "license": "MIT",
  "dependencies": {
    "ai-sandbox": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}
